        <boolProp name="resetInterpreter">false</boolProp>
        <stringProp name="script">String serverName = vars.get(&quot;jmeter.vars.httpRequest.serverName&quot;);
log.info(&quot;BEFORE: jmeter.vars.httpRequest.serverName: [&quot; + serverName + &quot;]&quot;);

//  Check if serverName value has not been filtered, i.e. it contains ${maven...
if(serverName.contains(&quot;${maven.&quot;)){
	log.info(&quot;The test plan is being executed in JMeter GUI and we must fall back to the default value&quot;);

	String defaultServerName = vars.get(&quot;jmeter.default.httpRequest.serverName&quot;);
	log.info(&quot;jmeter.default.httpRequest.serverName: [&quot; + defaultServerName + &quot;]&quot;);

	// Replace unfiltered value with default value
	vars.put(&quot;jmeter.vars.httpRequest.serverName&quot;, defaultServerName);

}

log.info(&quot;AFTER: jmeter.vars.httpRequest.serverName: [&quot; + vars.get(&quot;jmeter.vars.httpRequest.serverName&quot;) + &quot;]&quot;);</stringProp>
