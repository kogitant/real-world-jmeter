            <stringProp name="BeanShellAssertion.query">if (ResponseCode.equals(&quot;302&quot;) == true) { 
	log.info(&quot;Treating HTTP 302 as success&quot;);
	// We want to treat this code as SUCCESS instead of FAILURE
    	SampleResult.setSuccessful(true);
    	/*
    	Unlike in the SO answer, we do not want to change the response code from 302 to 200
    	SampleResult.setResponseCodeOK();
    	SampleResult.setResponseMessageOK();
    	*/
}</stringProp>
